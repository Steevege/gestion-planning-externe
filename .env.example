# === SUPABASE ===
# Public (sûr de mettre côté client car anon_key)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# Optionnel: Service role key (SECRETS - NE JAMAIS CÔTÉ CLIENT)
# SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# === NEXT.JS ===
# Node environment
NODE_ENV=development

# === OPTIONNEL: CONFIGURATION CALENDRIER ===
# Localisation pour React Big Calendar (moment, date-fns, etc.)
NEXT_PUBLIC_CALENDAR_LOCALE=fr

# === OPTIONNEL: PARAMÈTRES D'OPTIMISATION ===
# Timeout pour l'optimisation des schedules (en ms)
OPTIMIZATION_TIMEOUT=5000

# Niveau de détail du logging (debug, info, warn, error)
LOG_LEVEL=info
